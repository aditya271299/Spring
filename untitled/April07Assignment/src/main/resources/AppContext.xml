<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.w3.org/2001/XMLSchema"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="sAdd" class="com.bean.Address">
        <property name="city" value="Malkapur"/>
        <property name="state" value="Maharashtra"/>
    </bean>

    <bean id="st" class="com.bean.Student" autowire="constructor">
        <constructor-arg name="sId" value="1000"/>
        <constructor-arg name="sName" value="Aditya"/>


    </bean>

</beans>